{
  "plugins": ["react", "react-native"],

  "env": {
    "es6": true,
    "react-native/react-native": true
  },

  "rules": {
    "import/no-unresolved": "off",
    "react/sort-comp": "off",
    "no-unused-vars": 2,
    "jest/no-disabled-tests": "off",
    "react/jsx-uses-vars": ["error"]
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
